{
  "name": "@oxian/oxian-js",
  "version": "0.11.1",
  "exports": {
    ".": "./cli.ts",
    "./server": "./src/server/server.ts",
    "./config": "./src/config/types.ts",
    "./runtime": "./src/runtime/module_loader.ts",
    "./types": "./src/types.ts",
    "./mcp": "./src/utils/mcp.ts"
  },
  "publish": {
    "include": [
      "cli.ts",
      "llm.txt",
      "src/**",
      "deno.json",
      "docs/**",
      "README.md",
      "LICENSE*"
    ],
    "exclude": [
      "tests/**",
      "routes/**",
      "oxian-framework-spec.md"
    ]
  },
  "unstable": [
    "raw-imports",
    "bare-node-builtins",
    "detect-cjs",
    "node-globals",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "net",
    "otel"
  ],
  "imports": {
    "oxian-js/": "./src/",
    "@oxian/oxian-js/mcp": "jsr:@oxian/oxian-js@^0.1.0/mcp",
    "@std/path": "jsr:@std/path@1.1.1",
    "@std/cli/parse-args": "jsr:@std/cli@1.0.21/parse-args",
    "@std/jsonc/parse": "jsr:@std/jsonc@1.0.2/parse",
    "@std/encoding/base64": "jsr:@std/encoding@1.0.5/base64",
    "kv-toolbox/blob": "jsr:@kitsonk/kv-toolbox@0.30.0/blob"
  },
  "lock": false
}
