{
  "name": "@oxian/oxian-js",
  "version": "0.5.0",
  "exports": {
    ".": "./cli.ts",
    "./server": "./src/server/server.ts",
    "./config": "./src/config/types.ts",
    "./runtime": "./src/runtime/module_loader.ts"
  },
  "publish": {
    "include": [
      "cli.ts",
      "src/**",
      "deno.json",
      "README.md",
      "LICENSE*"
