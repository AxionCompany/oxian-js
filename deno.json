    "exclude": [
      "tests/**",
      "routes/**",
      "oxian-framework-spec.md"
    ]
  },
  "imports": {
    "@std/path": "jsr:@std/path",
    "@std/cli/parse-args": "jsr:@std/cli/parse-args",
    "@deno/emit": "jsr:@deno/emit@0.46.0"
  },
  "lock": false
} 